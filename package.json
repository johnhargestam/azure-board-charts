{
  "name": "azure-board-charts",
  "version": "1.0.0",
  "private": "true",
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc",
    "db:up": "docker compose -f db/docker-compose.yml up --wait",
    "db:down": "docker compose -f db/docker-compose.yml down",
    "format": "prettier -w .",
    "serve": "concurrently \"tsc --watch\" \"nodemon -q dist/app.js\"",
    "test:db": "yarn db:up && jest ./test/integration --runInBand ; yarn db:down",
    "test": "jest ./test/unit"
  },
  "dependencies": {
    "@js-joda/core": "^5.5.2",
    "azure-devops-node-api": "^11.2.0",
    "dotenv": "^16.0.3",
    "jest": "^29.5.0",
    "knex": "^2.4.2",
    "pg": "^8.10.0",
    "radash": "^10.8.1",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^18.11.3",
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
