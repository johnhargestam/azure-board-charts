{
  "name": "azure-board-charts",
  "version": "1.0.0",
  "private": "true",
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc",
    "format": "prettier -w .\\",
    "serve": "concurrently \"tsc --watch\" \"nodemon -q dist/app.js\"",
    "test:db": "jest --testPathPattern=integration --runInBand",
    "test:tc": "cross-env TESTCONTAINERS_RYUK_DISABLED=true jest --testPathPattern=integration --globalSetup ./test/fixtures/setup.ts --globalTeardown ./test/fixtures/teardown.ts --runInBand --detectOpenHandles --forceExit",
    "test": "jest --testPathPattern=unit"
  },
  "dependencies": {
    "@js-joda/core": "^5.5.2",
    "azure-devops-node-api": "^11.2.0",
    "dotenv": "^16.0.3",
    "jest": "^29.5.0",
    "knex": "^2.4.2",
    "tedious": "^16.0.0",
    "testcontainers": "^9.5.0",
    "ts-jest": "^29.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.3",
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
